{
  "name": "@triton-labs/wonka-cli",
  "version": "1.0.0",
  "description": "CLI utilities built around Wonka APIs.",
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "scripts": {
    "test": "test",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TritonLabs/wonka.git",
    "directory": "cli"
  },
  "keywords": [
    "wonka",
    "nft",
    "solana",
    "candy",
    "machine",
    "mint",
    "metaplex"
  ],
  "author": "@0xZoZoZo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TritonLabs/wonka/issues"
  },
  "homepage": "https://github.com/TritonLabs/wonka#readme",
  "dependencies": {
    "@project-serum/anchor": "^0.18.2",
    "@triton-labs/wonka": "^1.0.6",
    "commander": "^9.0.0",
    "install": "^0.13.0",
    "loglevel": "^1.8.0",
    "node-fetch": "^2.6.7",
    "npm": "^8.4.0",
    "typescript": "^4.5.5",
    "util": "^0.12.4"
  }
}
